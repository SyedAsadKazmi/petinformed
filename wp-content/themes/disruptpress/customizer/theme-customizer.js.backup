	// Header Logo Upload
	wp.customize( 'dp_primary_menu_logo_upload', function( value ) {
		value.bind( function( newval ) {
			
			if ( newval != '') {
				$( '.nav-primary .title-logo' ).remove();
				
				var logo_url = wp.customize.value( 'dp_primary_menu_logo_upload' )();
				var logo_height = wp.customize.value( 'dp_primary_menu_logo_height' )();
				
  			$( '.nav-primary .title-area' ).prepend( '<div class="title-logo"><img src="' + logo_url + '"></div>' ).find( 'img' ).css('height', logo_height + 'px');

			} else {
				$( '.nav-primary .title-logo' ).remove();
			}
			
		} );
	} );
	
