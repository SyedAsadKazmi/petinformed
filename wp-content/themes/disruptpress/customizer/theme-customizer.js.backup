wp.customize( 'dp_primary_menu_item_alignment', function( value ) {
		value.bind( function( newval ) {
			
			if ( newval == 'fullwidth') {
				$('.nav-primary .disruptpress-nav-menu').css('text-align', 'center');
				$('.nav-primary .disruptpress-nav-menu').css('display', 'flex');
				$('.nav-primary .disruptpress-nav-menu').css('justify-content', 'space-between');
				$('.nav-primary .disruptpress-nav-menu > li').css('width', '100%');
				$('.nav-primary .disruptpress-nav-menu > li > a').css('width', '100%');
			} else {
				$('.nav-primary .disruptpress-nav-menu').css('text-align', newval);
				$('.nav-primary .disruptpress-nav-menu').css('display', 'inline-block');
				$('.nav-primary .disruptpress-nav-menu').css('justify-content', 'flex-start');
				$('.nav-primary .disruptpress-nav-menu > li').css('width', 'auto');
				$('.nav-primary .disruptpress-nav-menu > li >  a').css('width', 'auto');
			}
		} );
	} );
	
