	function DPMenuIcons () {

		var dp_primary_menu_submenu_indicator = wp.customize.value( 'dp_primary_menu_submenu_indicator' )();
		var font_size = wp.customize.value( 'dp_primary_menu_font_size' )();
		//var font_size_home_icon = parseInt(font_size) + 14;
		var font_size_submenu = Math.round( ( parseInt(font_size) * 0.5 ) + 12 );

		if ( dp_primary_menu_submenu_indicator === false ) {

			$( "head" ).append( '<style type="text/css">\
	.nav-primary .disruptpress-nav-menu > .menu-item-has-children > a:after {\
		display: none; }\
	</style>' );
		}

		if ( dp_primary_menu_submenu_indicator === true ) {

			$( "head" ).append( '<style type="text/css">\
	.nav-primary .disruptpress-nav-menu > .menu-item-has-children > a:after {\
	font-size: ' + font_size_submenu + 'px;\
		line-height: ' + font_size + 'px;\
	display: inline-block; }\
	 </style>' );
		}

	}

