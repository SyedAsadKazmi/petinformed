	function dpMenuItemDividers ( selector, color_selector, color2_selector, colortoggle_in, style_in, open_in, close_in, top_in, bottom_in, search_toggle, search_open, search_close, search_selector, boxshadow ) {
		
		var color = dp_rgb2hex( wp.customize.value( color_selector )() );
		var color2 = dp_rgb2hex( wp.customize.value( color2_selector )() );
		var style = wp.customize.value( style_in )();
		var open = wp.customize.value( open_in )();
		var close = wp.customize.value( close_in )();
		var top = wp.customize.value( top_in )();
		var bottom = wp.customize.value( bottom_in )();
		var colortoggle = wp.customize.value( colortoggle_in )();
		var search = wp.customize.value( search_toggle )();
		var search_open = wp.customize.value( search_open )();
		var search_close = wp.customize.value( search_close )();
		var boxshadow = wp.customize.value( boxshadow )();

		if ( colortoggle == false ) {
			var newcolor = shading( color, -0.4);
			var newcolor2 = shading( color, 0.3);
			
		} else {
			var newcolor = shading( color2, -0.4);
			var newcolor2 = shading( color2, 0.3);
		}
		
		if ( search == '0' ) {
			var selector_last = ':last-child';
		} else {
			var selector_last = ':nth-last-child(2)';
		}
		
		if ( style == '0' ) {
			$( selector ).css('box-shadow', 'none');
			$( selector + ':first-child' ).css('box-shadow', 'none');
			$( selector + ':last-child' ).css('box-shadow', 'none');
			
			$( selector + ':first-child' ).css('border-left', 'none');
			$( selector + ':last-child' ).css('border-right', 'none');
			
			$( selector ).css('border-top', 'none');
			$( selector ).css('border-bottom', 'none');
		} 
			
		if ( style == '1' ) {
			$( selector ).css('box-shadow', 'none');	
			
			$( selector ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset');
			//$( selector + ':first-child' ).css('box-shadow', 'none');
			
			if ( open == true ) {
				$( selector + ':first-child' ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset');
				
			} else {
				$( selector + ':first-child' ).css('box-shadow', 'none');
			}
			
			if ( close == true ) {
				$( selector + selector_last ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor + ' -1px 0px 0px 0px inset');
			} else {
				$( selector + selector_last ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset');
			}
			
			if ( search == '1' ) {
			
				if ( search_open == true && search_close == true ) {
					$( search_selector ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor + ' -1px 0px 0px 0px inset');
				} else if ( search_open == true ) {
					$( search_selector ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset');
				} else if ( search_close == true ) {
					$( search_selector ).css('box-shadow', newcolor + ' -1px 0px 0px 0px inset');
				} else {
					$( search_selector ).css('box-shadow', 'none');
				}
				
			}
			
			if ( top == true ) {
				$( selector ).css('border-top', '1px solid ' + newcolor);
			} else {
				$( selector ).css('border-top', 'none');
			}
			
			if ( bottom == true ) {
				$( selector ).css('border-bottom', '1px solid ' + newcolor );
			} else {
				$( selector ).css('border-bottom', 'none');
			}
			
		}
		
		if ( style == '2' ) {
			$( selector ).css('box-shadow', 'none');	

			if ( top == true ) {
				$( selector ).css('border-top', '1px solid ' + newcolor );
				var border_top = ', ' + newcolor2 + ' 0px 1px 0px 0px inset';
			} else {
				$( selector ).css('border-top', 'none');
				var border_top = '';
			}
			
			if ( bottom == true ) {
				$( selector ).css('border-bottom', '1px solid ' + newcolor );
			} else {
				$( selector ).css('border-bottom', 'none');
			}
			
			if ( boxshadow == '0') {
				$( selector ).css('box-shadow', newcolor + ' -1px 0px 0px 0px, ' + newcolor2 + ' -2px 0px 0px 0px' + border_top );
			} else {
				$( selector ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor2 + ' 2px 0px 0px 0px inset' + border_top );
			}
			
			if ( open == true ) {
				if ( boxshadow == '0') {
				 $( selector ).css('box-shadow', newcolor + ' -1px 0px 0px 0px, ' + newcolor2 + ' -2px 0px 0px 0px' + border_top );
				} else {
					$( selector ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor2 + ' 2px 0px 0px 0px inset' + border_top );
				}
				
			} else {
				$( selector + ':first-child' ).css('box-shadow', 'none');
			}
			
			if ( close == true ) {
				
				if ( boxshadow == '0') {
					$( selector + selector_last ).css('box-shadow', newcolor + ' -1px 0px 0px 0px, ' + newcolor2 + ' -2px 0px 0px 0px, ' + newcolor2 + ' 1px 0px 0px 0px, ' + newcolor + ' 2px 0px 0px 0px' + border_top );
				} else {
					$( selector + selector_last ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor2 + ' 2px 0px 0px 0px inset, ' + newcolor2 + ' -1px 0px 0px 0px inset, ' + newcolor + ' -2px 0px 0px 0px inset' + border_top );
				}
				
			} else {
				$( selector + selector_last ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor2 + ' 2px 0px 0px 0px inset' + border_top );
			//	$( selector + ':last-child' ).css('box-shadow', newcolor2 + ' 1px 0px 0px 0px inset' + border_top );
			}
			
			if ( search == '1' ) {
			
				if ( search_open == true && search_close == true ) {
					
					$( search_selector ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor2 + ' 2px 0px 0px 0px inset, ' + newcolor2 + ' -1px 0px 0px 0px inset, ' + newcolor + ' -2px 0px 0px 0px inset' + border_top );
				} else if ( search_open == true ) {
					$( search_selector ).css('box-shadow', newcolor + ' 1px 0px 0px 0px inset, ' + newcolor2 + ' 2px 0px 0px 0px inset' + border_top );
				} else if ( search_close == true ) {
					$( search_selector ).css('box-shadow', newcolor + ' -1px 0px 0px 0px inset, ' + newcolor2 + ' -2px 0px 0px 0px inset' + border_top );
				} else {
					$( search_selector ).css('box-shadow', 'none');
				}
				
			}
		}
	}
	
	
	// Boxed, Wide or Full Width
	wp.customize( 'dp_site_layout', function( value ) {
		value.bind( function( newval ) {
			var container_width = wp.customize.value( 'dp_site_container_width' )();
			var padding_top_bottom = wp.customize.value( 'dp_site_container_margin_top_bottom' )();

			if(newval == '1') {
			
				$('.site-container').css('max-width', container_width + 'px' );
				$('.wrap').css('max-width', container_width + 'px' );
				//$('.site-inner').css('max-width', container_width + 'px' );
				
				//$('.dp_body_wrap_1').css('padding-top', padding_top_bottom + 'px' );
				//$('.dp_body_wrap_1').css('padding-bottom', padding_top_bottom + 'px' );
			}
			
			if(newval == '2') {
				
				$('.site-container').css('max-width', '100%' );
				$('.wrap').css('max-width', container_width + 'px' );	
				//$('.site-inner').css('max-width', container_width + 'px' );
				
// 				$('.dp_body_wrap_1').css('padding-top', '0px' );
// 				$('.dp_body_wrap_1').css('padding-bottom', '0px' );
			}
			
			if(newval == '3') {
				
				$('.site-container').css('max-width', '100%' );	
				$('.wrap').css('max-width', '100%' );
				//$('.site-inner').css('max-width', '100%' );
				
// 				$('.dp_body_wrap_1').css('padding-top', '0px' );
// 				$('.dp_body_wrap_1').css('padding-bottom', '0px' );
			}
			
		} );
	} );

