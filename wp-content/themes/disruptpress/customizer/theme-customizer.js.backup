	wp.customize( 'dp_primary_menu_bg_color_style', function( value ) {
		value.bind( function( newval ) {
			
			var items_only = wp.customize.value( 'dp_primary_menu_bg_items_only' )();
			
			if ( items_only == true ) {
				$('.nav-primary').css('background', 'transparent' );
				apply_bg_no_img('.nav-primary .disruptpress-nav-menu > li', 'dp_primary_menu_bg');
				
			} else {
				$('.nav-primary .disruptpress-nav-menu > li').css('background', 'transparent' );
				apply_bg_no_img('.nav-primary ', 'dp_primary_menu_bg');
			}
			
		} );
	} );
	
